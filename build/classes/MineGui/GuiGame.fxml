<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="524.0" prefWidth="748.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MineGui.GuiGameController">
    <stylesheets>
        <URL value="@guigame.css" />
    </stylesheets>
   <children>
      <ImageView id="logoimage" fitHeight="150.0" fitWidth="179.0" layoutX="66.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Logoswepper.jpg" />
         </image>
      </ImageView>
      <Button fx:id="myNewGameButton" alignment="CENTER" layoutX="24.0" layoutY="254.0" mnemonicParsing="false" onAction="#newGameButton" prefHeight="41.0" prefWidth="234.0" text="Human VS. Human" textAlignment="CENTER">
         <font>
            <Font name="Forte" size="22.0" />
         </font>
      </Button>
      <Button alignment="CENTER" layoutX="24.0" layoutY="451.0" mnemonicParsing="false" onAction="#OptionMenu" prefHeight="41.0" prefWidth="234.0" styleClass="mainmenubuttons" text="Options" textAlignment="CENTER">
         <font>
            <Font name="Forte" size="22.0" />
         </font>
      </Button>
      <Button fx:id="myNewGameButton1" alignment="CENTER" layoutX="24.0" layoutY="321.0" mnemonicParsing="false" onAction="#playvspc" prefHeight="41.0" prefWidth="234.0" styleClass="mainmenubuttons" text="PC VS. Human" textAlignment="CENTER">
         <font>
            <Font name="Forte" size="22.0" />
         </font>
      </Button>
      <Button fx:id="myNewGameButton2" alignment="CENTER" layoutX="24.0" layoutY="187.0" mnemonicParsing="false" onAction="#SinglePlayer" prefHeight="41.0" prefWidth="234.0" styleClass="mainmenubuttons" text="Single Player" textAlignment="CENTER">
         <font>
            <Font name="Forte" size="22.0" />
         </font>
      </Button>
      <Button alignment="CENTER" layoutX="24.0" layoutY="386.0" mnemonicParsing="false" onAction="#load" prefHeight="41.0" prefWidth="234.0" styleClass="mainmenubuttons" text="Load Game" textAlignment="CENTER">
         <font>
            <Font name="Forte" size="22.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
